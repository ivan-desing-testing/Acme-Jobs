<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="conversion-service.xml" />

	<!-- Shouts -->
	<bean id="shout01" class ="acme.entities.shouts.Shout">
		<property name ="author" value="John Does"/>
		<property name ="text" value="I'm happy"/>
		<property name ="moment" value="2019/09/09 10:00"/>
	</bean>
	
	<bean id="shout02" class ="acme.entities.shouts.Shout">
		<property name ="author" value="Jane Doe"/>
		<property name ="text" value="I'm the master of my room"/>
		<property name ="moment" value="2019/08/09 15:55"/>
	</bean>
	
	<bean id="shout03" class ="acme.entities.shouts.Shout">
		<property name ="author" value="Foo Bar"/>
		<property name ="text" value="Just turning 20!"/>
		<property name ="moment" value="2019/09/07 00:00"/>
	</bean>
	
	<!-- CardenasBulletin -->
	
	<bean id="cardenasBulletin01" class ="acme.entities.cardenasBulletin.CardenasBulletin">
		<property name ="book" value="La Comunidad del Anillo"/>
		<property name ="author" value="J.R.R Tolkien"/>
		<property name ="genre" value="Fantasía"/>
		<property name ="isbn" value="7312310239"/>
	</bean>
	
	<bean id="cardenasBulletin02" class ="acme.entities.cardenasBulletin.CardenasBulletin">
		<property name ="book" value="Las Dos Torres"/>
		<property name ="author" value="J.R.R Tolkien"/>
		<property name ="genre" value="Fantasía"/>
		<property name ="isbn" value="801123412"/>
	</bean>
	
	<bean id="cardenasBulletin03" class ="acme.entities.cardenasBulletin.CardenasBulletin">
		<property name ="book" value="The Final Empire"/>
		<property name ="author" value="Brandon Sanderson"/>
		<property name ="genre" value="Fantasía"/>
		<property name ="isbn" value="134523452"/>
	</bean>
	
	
	
</beans>


